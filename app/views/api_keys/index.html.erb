<h1>Listing api_keys</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Key</th>
  </tr>

<% @api_keys.each do |api_key| %>
  <tr>
    <td><%=h api_key.username %></td>
    <td><%=h api_key.key %></td>
    <td><%= link_to 'Show', api_key %></td>
    <td><%= link_to 'Edit', edit_api_key_path(api_key) %></td>
    <td><%= link_to 'Destroy', api_key, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New api_key', new_api_key_path %>